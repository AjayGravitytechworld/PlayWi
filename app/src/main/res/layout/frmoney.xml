<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.circularreveal.CircularRevealRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/allbg"
    tools:context="com.abc.demo.fragment.VideoFragm">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/_10dp"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/SpinBtn"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_140dp"
                android:layout_gravity="center"
                android:layout_marginHorizontal="@dimen/_8dp"
                android:layout_weight="1"
                android:background="@drawable/spinwheelbg"
                android:gravity="center"
                android:orientation="vertical">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tex_spin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_14dp"
                    android:layout_marginTop="@dimen/_8dp"
                    android:gravity="center"
                    android:text="@string/str_spinWheel"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_18dp"
                    android:textStyle="bold" />

                <com.google.android.material.circularreveal.CircularRevealRelativeLayout
                    android:layout_width="@dimen/_85dp"
                    android:layout_height="@dimen/_32dp"
                    android:layout_below="@+id/tex_spin"
                    android:layout_marginStart="@dimen/_14dp"
                    android:layout_marginTop="@dimen/_10dp"
                    android:background="@drawable/btnbg">

                    <com.google.android.material.textview.MaterialTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:gravity="center"
                        android:text="@string/str_spinbtn"
                        android:textColor="#264999"
                        android:textSize="@dimen/_14dp"
                        android:textStyle="bold" />
                </com.google.android.material.circularreveal.CircularRevealRelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/qwe"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_140dp"
                android:layout_gravity="center"
                android:layout_marginHorizontal="@dimen/_8dp"
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_weight="1"
                android:background="@drawable/ratebtnbg"
                android:gravity="center"
                android:orientation="vertical">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tex_rate1122"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_marginTop="@dimen/_8dp"
                    android:text="@string/str_visit_earn"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_18dp"
                    android:textStyle="bold" />

                <com.google.android.material.circularreveal.CircularRevealRelativeLayout
                    android:layout_width="@dimen/_85dp"
                    android:layout_height="@dimen/_32dp"
                    android:layout_marginStart="@dimen/_12sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btnbg2">

                    <com.google.android.material.textview.MaterialTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:gravity="center"
                        android:text="@string/str_visit_earn1"
                        android:textColor="#7232F1"
                        android:textSize="@dimen/_14dp" />
                </com.google.android.material.circularreveal.CircularRevealRelativeLayout>
            </LinearLayout>

            <com.google.android.material.circularreveal.CircularRevealLinearLayout
                android:id="@+id/nativeAd"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5dp"
                android:orientation="vertical" />

            <LinearLayout
                android:id="@+id/quiz_btn"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_140dp"
                android:layout_gravity="center"
                android:layout_marginHorizontal="@dimen/_8dp"
                android:layout_marginTop="10sp"
                android:layout_weight="1"
                android:background="@drawable/quizbg"
                android:gravity="center"
                android:orientation="vertical">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tex_quiz"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_marginTop="@dimen/_8dp"
                    android:text="@string/str_quiz"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_18dp"
                    android:textStyle="bold" />

                <com.google.android.material.circularreveal.CircularRevealRelativeLayout
                    android:layout_width="@dimen/_85dp"
                    android:layout_height="@dimen/_32dp"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btnbg3">

                    <com.google.android.material.textview.MaterialTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:gravity="center"
                        android:text="@string/str_ansbtn"
                        android:textColor="#E43686"
                        android:textSize="@dimen/_14sdp" />
                </com.google.android.material.circularreveal.CircularRevealRelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/scratchBtn"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_140dp"
                android:layout_marginHorizontal="@dimen/_8dp"
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_weight="1"
                android:background="@drawable/scretchbg"
                android:gravity="center"
                android:orientation="vertical">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tex_scratch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_marginTop="@dimen/_8dp"
                    android:text="@string/str_scratch"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_18dp"
                    android:textStyle="bold" />

                <com.google.android.material.circularreveal.CircularRevealRelativeLayout
                    android:layout_width="@dimen/_85dp"
                    android:layout_height="@dimen/_32dp"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btnbg4">

                    <com.google.android.material.textview.MaterialTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:gravity="center"
                        android:text="@string/str_spinbtn"
                        android:textColor="#F85162"
                        android:textSize="@dimen/_14sdp" />
                </com.google.android.material.circularreveal.CircularRevealRelativeLayout>
            </LinearLayout>





            <LinearLayout
                android:id="@+id/slot_btn"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_140dp"
                android:layout_gravity="center"
                android:layout_marginHorizontal="@dimen/_8dp"
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_weight="1"
                android:background="@drawable/slotbg"
                android:gravity="center"
                android:orientation="vertical">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tex_slot"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_marginTop="@dimen/_8dp"
                    android:text="@string/str_slot"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_18dp"
                    android:textStyle="bold" />

                <com.google.android.material.circularreveal.CircularRevealRelativeLayout
                    android:layout_width="@dimen/_85dp"
                    android:layout_height="@dimen/_32dp"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btnbg7">

                    <com.google.android.material.textview.MaterialTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:gravity="center"
                        android:text="@string/str_slotbtn"
                        android:textColor="#EA2F2F"
                        android:textSize="@dimen/_14sdp" />
                </com.google.android.material.circularreveal.CircularRevealRelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/rate_btn11"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_140dp"
                android:layout_gravity="center"
                android:layout_marginHorizontal="@dimen/_8dp"
                android:layout_marginTop="@dimen/_15sdp"
                android:background="@drawable/ratebtnbg123"
                android:gravity="center"
                android:orientation="vertical">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tex_rate11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_marginTop="@dimen/_8dp"
                    android:text="@string/str_visit_earn"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_18dp"
                    android:textStyle="bold" />

                <com.google.android.material.circularreveal.CircularRevealRelativeLayout
                    android:layout_width="@dimen/_85dp"
                    android:layout_height="@dimen/_32dp"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btnbg8">

                    <com.google.android.material.textview.MaterialTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:gravity="center"
                        android:text="@string/str_visit_earn1"
                        android:textColor="#415CFA"
                        android:textSize="@dimen/_14sdp" />
                </com.google.android.material.circularreveal.CircularRevealRelativeLayout>
            </LinearLayout>


            <com.google.android.material.circularreveal.CircularRevealLinearLayout
                android:id="@+id/nativeAd2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5dp"
                android:orientation="vertical" />

            <LinearLayout
                android:id="@+id/rate_btn"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_140dp"
                android:layout_gravity="center"
                android:layout_marginHorizontal="@dimen/_8dp"
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_weight="1"
                android:background="@drawable/ratebtnbg1"
                android:gravity="center"
                android:orientation="vertical">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tex_rate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_marginTop="@dimen/_8dp"
                    android:text="@string/str_rateus"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_18dp"
                    android:textStyle="bold" />

                <com.google.android.material.circularreveal.CircularRevealRelativeLayout
                    android:layout_width="@dimen/_85dp"
                    android:layout_height="@dimen/_32dp"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btnbg9">

                    <com.google.android.material.textview.MaterialTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:gravity="center"
                        android:text="@string/str_ratebtn"
                        android:textColor="#007699"
                        android:textSize="@dimen/_14sdp" />
                </com.google.android.material.circularreveal.CircularRevealRelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/invite_btn"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_140dp"
                android:layout_marginHorizontal="@dimen/_8dp"
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_weight="1"
                android:background="@drawable/invitebtnbg"
                android:gravity="center"
                android:orientation="vertical">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tex_invite"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_marginTop="@dimen/_8dp"
                    android:text="@string/str_invite"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_18dp"
                    android:textStyle="bold" />

                <com.google.android.material.circularreveal.CircularRevealRelativeLayout
                    android:layout_width="@dimen/_85dp"
                    android:layout_height="@dimen/_32dp"
                    android:layout_marginStart="@dimen/_14sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btnbg1">

                    <com.google.android.material.textview.MaterialTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:gravity="center"
                        android:text="@string/str_invitebtn"
                        android:textColor="#0A9E60"
                        android:textSize="@dimen/_14sdp" />
                </com.google.android.material.circularreveal.CircularRevealRelativeLayout>
            </LinearLayout>

        </androidx.appcompat.widget.LinearLayoutCompat>
    </androidx.core.widget.NestedScrollView>
</com.google.android.material.circularreveal.CircularRevealRelativeLayout>